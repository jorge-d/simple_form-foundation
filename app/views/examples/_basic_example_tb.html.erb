<form role="form">
  <div class="row">
    <div class="large-12 columns">
      <label>Email
        <input type="email" placeholder="Enter email">
      </label>
    </div>
  </div>

  <div class="row">
    <div class="large-12 columns">
      <label>Password
        <input type="password" placeholder="Password">
      </label>
    </div>
  </div>

  <div class="row">
    <div class="large-12 columns">
      <label>File
        <input type="file">
      </label>
    </div>
  </div>

  <div class="row">
    <div class="large-12 columns"><input type="checkbox" id="checkbox1" /><label for="checkbox1" class="boolean" >Active</label></div>
  </div>

  <div class="row">
    <div class="large-12 columns">
      <label>Choices</label>

      <div class="checkbox">
        <input type="checkbox" id="checkbox2" name="optionsRadios" value="option1" checked /><label for="checkbox2">Option one is this and that&mdash;be sure to include why it's great
        </label>
      </div>
      <div class="checkbox">
        <input type="checkbox" id="checkbox3" name="optionsRadios" value="option2" /><label for="checkbox3">Option two can be something else</label>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="large-12 columns">
      <label>Sex</label>
      <div class="radio">
        <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked /><label for="optionsRadios1">Male</label>
      </div>
      <div class="radio">
        <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2" /><label for="optionsRadios2">Female</label>
      </div>
    </div>
  </div>

  <button type="submit" class="btn btn-default">Create User</button>
</form>
