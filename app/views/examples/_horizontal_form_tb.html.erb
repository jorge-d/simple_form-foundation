<form role="form">
  <div class="row">
    <div class="small-3 columns">
      <label for="inputEmail3" class="right inline">Email</label>
    </div>
    <div class="small-9 columns">
      <input type="email" id="inputEmail3" placeholder="Email">
    </div>
  </div>

  <div class="row">
    <div class="small-3 columns">
      <label for="inputPassword3" class="right inline">Password</label>
    </div>
    <div class="small-9 columns">
      <input type="password" id="inputPassword3" placeholder="Password">
    </div>
  </div>

  <div class="row">
    <div class="small-3 columns">
      <label for="exampleInputFile" class="right inline">File</label>
    </div>
    <div class="small-9 columns">
      <input type="file" id="exampleInputFile">
      <span class="hint">Define your own hint style</span>
    </div>
  </div>

  <div class="row">
    <div class="small-offset-3 small-9 columns">
      <label class="boolean">
        <input type="checkbox">Active
      </label>
    </div>
  </div>

  <div class="row">
    <div class="small-3 columns">
      <label class="right inline">Choices</label>
    </div>
    <div class="small-9 columns">
      <div class="checkbox">
        <label>
          <input type="checkbox" name="optionsRadios" id="optionsCheckbox1" value="option1" checked>Option one is this and that&mdash;be sure to include why it's great
        </label>
      </div>
      <div class="checkbox">
        <label>
          <input type="checkbox" name="optionsRadios" id="optionsCheckbox2" value="option2">Option two can be something else and selecting it will deselect option one
        </label>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="small-3 columns">
      <label class="right inline">Sex</label>
    </div>
    <div class="small-9 columns">
      <div class="radio">
        <label>
          <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>Male
        </label>
      </div>
      <div class="radio">
        <label>
          <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2">Female
        </label>
      </div>
    </div>
  </div>

  <div class='row'>
    <div class="small-offset-3 small-9 columns">
      <button type="submit" class="btn btn-default">Create User</button>
    </div>
  </div>
</form>

